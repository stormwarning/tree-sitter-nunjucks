{
	"name": "tree-sitter-nunjucks",
	"version": "0.1.0",
	"description": "Nunjucks grammar for tree-sitter",
	"keywords": [
		"incremental",
		"parsing",
		"tree-sitter",
		"nunjucks"
	],
	"repository": "https://github.com/stormwarning/tree-sitter-nunjucks",
	"license": "ISC",
	"author": "Jeff Nelson (https://tidaltheory.io)",
	"main": "bindings/node",
	"types": "bindings/node",
	"files": [
		"grammar.js",
		"tree-sitter.json",
		"binding.gyp",
		"prebuilds/**",
		"bindings/node/*",
		"queries/*",
		"src/**",
		"*.wasm"
	],
	"scripts": {
		"build": "tree-sitter generate",
		"install": "node-gyp-build",
		"lint": "eslint grammar.js",
		"prestart": "tree-sitter build --wasm",
		"start": "tree-sitter playground",
		"test": "tree-sitter test",
		"test:bindings": "node --test bindings/node/*_test.js",
		"test:grammar": "tree-sitter test"
	},
	"prettier": "@zazen/prettier-config",
	"dependencies": {
		"node-addon-api": "8.5.0",
		"node-gyp-build": "4.8.4"
	},
	"devDependencies": {
		"@zazen/prettier-config": "1.1.3",
		"eslint": "9.31.0",
		"eslint-config-treesitter": "1.0.2",
		"prebuildify": "6.0.1",
		"prettier": "3.6.2",
		"tree-sitter-cli": "0.26.5"
	},
	"peerDependencies": {
		"tree-sitter": "^0.25.0"
	},
	"peerDependenciesMeta": {
		"tree-sitter": {
			"optional": true
		}
	}
}
