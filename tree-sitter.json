{
	"$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
	"grammars": [
		{
			"name": "nunjucks",
			"camelcase": "Nunjucks",
			"title": "Nunjucks",
			"scope": "source.nunjucks",
			"file-types": ["njk"],
			"injection-regex": "^nunjucks$",
			"class-name": "TreeSitterNunjucks"
		}
	],
	"metadata": {
		"version": "0.1.0",
		"license": "ISC",
		"description": "Nunjucks grammar for tree-sitter",
		"authors": [
			{
				"name": "Jeff Nelson",
				"url": "https://tidaltheory.io/"
			}
		],
		"links": {
			"repository": "https://github.com/stormwarning/tree-sitter-nunjucks"
		}
	},
	"bindings": {
		"c": true,
		"go": true,
		"node": true,
		"python": true,
		"rust": true,
		"swift": true,
		"zig": false
	}
}
